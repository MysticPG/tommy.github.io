---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>arganzheng's Weblog</title>
 <link href="http://kyle.xlau.org/atom.xml" rel="self"/>
 <link href="http://kyle.xlau.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://github.io/tommy.github.io/</id>
 <author>
   <name>tommy yang</name>
   <email>tingzai.yang@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://github.io/tommy.github.io/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://github.io/tommy.github.io/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
